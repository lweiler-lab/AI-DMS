<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Security Groups (V19: category_id and users removed from res.groups) -->
    <record id="group_dms_property_user" model="res.groups">
        <field name="name">DMS Property User</field>
        <field name="implied_ids" eval="[(4, ref('documents.group_documents_user'))]"/>
    </record>

    <record id="group_dms_property_manager" model="res.groups">
        <field name="name">DMS Property Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_dms_property_user'))]"/>
    </record>

    <record id="group_dms_property_admin" model="res.groups">
        <field name="name">DMS Property Administrator</field>
        <field name="implied_ids" eval="[(4, ref('group_dms_property_manager'))]"/>
    </record>
</odoo>
